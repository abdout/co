{
  "version": 2,
  "buildCommand": "pnpm vercel-build",
  "installCommand": "pnpm install",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "devCommand": "pnpm dev",
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  },
  "env": {
    "NEXT_PUBLIC_VERCEL_ENV": "production",
    "PRISMA_BINARY_TARGETS": "rhel-openssl-3.0.x",
    "NODE_ENV": "production"
  }
} 