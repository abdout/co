{
    "rewrites": [
      {
        "source": "/blocks/:path*",
        "destination": "/:path*"
      },
      {
        "source": "/blocks/_next/:path*",
        "destination": "/_next/:path*"
      }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "X-Frame-Options",
            "value": "DENY"
          },
          {
            "key": "X-Content-Type-Options",
            "value": "nosniff"
          },
          {
            "key": "Referrer-Policy",
            "value": "strict-origin-when-cross-origin"
          }
        ]
      }
    ]
  }